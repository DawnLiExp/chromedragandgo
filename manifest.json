{
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "dragandgo.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "permissions": [
        "storage",
        "cookies",
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "__MSG_extDescription__",
  "name": "__MSG_extName__",
  "icons": {
    "32": "dragandgo_32x32.png",
    "48": "dragandgo_48x48.png",
    "128": "dragandgo_128x128.png"
  },
  "options_page": "options.html",
  "version": "2.0.5",
  "default_locale": "en"
}